--TEST--
simdjson_encode() escape
--FILE--
<?php
for ($i = 0; $i < 127; $i++) {
    echo "$i: ";
    $original = json_encode(chr($i), JSON_UNESCAPED_LINE_TERMINATORS | JSON_UNESCAPED_SLASHES | JSON_UNESCAPED_UNICODE);
    $simdjson = simdjson_encode(chr($i));
    if ($original !== $simdjson) {
        echo "Different!!! Original: $original, simdjson $simdjson\n";
    }
    var_dump($simdjson);
}
?>
